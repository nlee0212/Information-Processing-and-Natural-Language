   "source": [
    "words = nltk.corpus.brown.tagged_words(categories='news',\n",
    "                                      tagset='universal')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 25,
   "metadata": {},
   "outputs": [],
   "source": [
    "verbs = [k for k,v in nltk.FreqDist(w for w,tag in words if tag=='VERB').most_common(10)]"
